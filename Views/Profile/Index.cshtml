@model IEnumerable<ProfileViewModel>

<div>
    <h1>Hello, World. This is Profile Page</h1>

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Surname</th>
                <th scope="col">Date of Birth</th>
                <th scope="col">Age</th>
                <th scope="col">Country</th>
                <th scope="col">City</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Name</td>
                    <td>@user.Surname</td>
                    <td>@user.DOB</td>
                    <td>@user.Age</td>
                    <td>@user.Country</td>
                    <td>@user.City</td>
                </tr>
            }
        </tbody>
    </table>
</div>

